{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.symbol.wellknown"
    ],
    "inlineSourceMap": true,
    "outDir": "../build",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": [
      "intern",
      "kendo-ui",
      "jquery"
    ]
  },
  "include": [
    "**/*.ts",
    "../kendo-multi-calendar.ts"
  ]
}